import java.util.*;
public class Main
{
	public static void main(String[] args) {
 
    Scanner Sc=new Scanner(System.in);
    int limit=Sc.nextInt();
    for(int row=1;row<2*limit;row++, System.out.println()){
        for(int col=1;col<2*limit;col++)
    System.out.print(Math.min(Math.min(row,col),Math.min(2*limit-row,2*limit-col)));
    }
}
}

		
**************************Reverse value pattern**************************

import java.util.*;
public class Main
{
	public static void main(String[] args) {
 
    Scanner Sc=new Scanner(System.in);
    int limit=Sc.nextInt();
    for(int row=1;row<2*limit;row++, System.out.println()){
        for(int col=1;col<2*limit;col++)
    System.out.print(Math.max(Math.max(row,col),Math.max(2*limit-row,2*limit-col))-limit+1);
    }
}
}

	

12.input=5
 1   2  3  4 5
 10  9  8  7 6
 11 12 13 14 15
 20 19 18 17 16
 21 22 23 24 25
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int num = 1;
        
        for (int row = 1; row <= n; row++, System.out.println()) {
            if (row % 2 != 0) {
                for (int col = 1; col <= n; col++) 
                    System.out.printf("%2d ",num++);
            } 
	    else {
                int start = num + n - 1;
                for (int col = 1; col <= n; col++) 
                    System.out.printf("%2d ",start--);
                           num += n;
            }
        }
    }
}


OR
public static void main(String[] args)
{
    Scanner in=new Scanner(System.in);
    int n=in.nextInt();
    int lim=0;
    for(int row=1;row<=n;row++,System.out.println())
    {
        if(row%2==1)
        {
            for(int col=lim+1;col<=lim+n;col++)
            {
                System.out.print(col+" ");
            }
            lim+=2*n;
        }
        else
        {
            for(int col=lim;col>lim-n;col--)
            {
                System.out.print(col+" ");
            }
        }
    }
    }
}


