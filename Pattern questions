import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner ob = new Scanner(System.in);
        int len= ob.nextInt();
        int bre=ob.nextInt();
        for (int row = 1; row <= len; row++, System.out.println()) {
            for (int col = 1; col <=bre; col++) 
            {
            if(row==1 || row==len || col==1 || col==bre)   System.out.print("*");
                else
                System.out.print(" ");
            }
            
        }

    ob.close();     }
}


17.Heart shape n=6
 ***   *** 
***** ***** 
***********
 ********* 
   *****   
    ***    
     *     

import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
              int n = scanner.nextInt();
        

        for (int row = n / 2; row <= n; row += 2) {
            for (int col = 1; col < n - row; col += 2)
                System.out.print(" ");
            
            for (int col = 1; col <= row; col++) 
                System.out.print("*");
            
            for (int col = 1; col <= n -row; col++) 
                System.out.print(" ");
            
            for (int col = 1; col <= row; col++) 
                System.out.print("*");
            
            System.out.println();
        }
        
        for (int row = n; row >0; row--) {
            for (int col = row; col < n; col++) {
                System.out.print(" ");
            }
            for (int col = 1; col <= (row * 2) - 1; col++) {
                System.out.print("*");
            }
            System.out.println();
        }
    }
}

18.Hour class n=5
*********
 *******
