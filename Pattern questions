public static void main(String[] args)
{
    System.out.print("Enter number of rows : ");
    Scanner in=new Scanner(System.in);
    int n=in.nextInt();
    for(int row=1;row<=n;row++,System.out.println())
    {
        for(int col=1;col<=row;col++)
        {
            System.out.print(col%2);
        }
    }
    }
}

