import java.util.*;
class Main{
    public static void main(String args[]){
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    for(int row=1;row<=n;row++,System.out.println()){
        for(int col=1;col<=n;col++)
        System.out.print(col);
    }
}
}
